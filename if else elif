name = input("Insert your name: ")
name = name.lower() #make it lower or upper so every input will be readable by the programs

age = input("Insert your age: ")
age = int(age)

if name == 'david' and age == 20:
    print("Welcome home, sir!")
elif name != 'david' and age >=21:
    print("You're too old, strangers.")
elif name != 'david' and age <=21:
    print("You're too young, kiddos.")
elif name == 'david' and age >=21:
    print("You're too old, David.")
elif name == 'david' and age <=21:
    print("You're too young, David.")
else:
    print("You're not identified.")

